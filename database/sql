-- -----------------------------------------------------
-- Table `cadastro_unico`.`especificacoes_tecnicas`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `cadastro_unico`.`especificacoes_tecnicas` (
  `idEspecificacao` INT(11) NOT NULL AUTO_INCREMENT,
  `idProduto` INT(11) NOT NULL,
  `idTipoEspecificacao` INT(11) NOT NULL,
  `componente` VARCHAR(50) NULL DEFAULT '0',
  `formatoAberto` VARCHAR(100) NULL DEFAULT '0',
  `formatoFechado` VARCHAR(100) NULL DEFAULT '0',
  `numPagina` INT(11) NULL DEFAULT '0',
  `papel` VARCHAR(100) NULL DEFAULT '0',
  `cores` VARCHAR(150) NULL DEFAULT '0',
  `acabamento` VARCHAR(150) NULL DEFAULT '0',
  `observacoes` VARCHAR(300) NULL DEFAULT '0',
  `espessura` VARCHAR(50) NULL DEFAULT '0',
  `peso` DOUBLE NULL DEFAULT '0',
  `orientacao` VARCHAR(50) NULL DEFAULT NULL,
  `alvura` VARCHAR(50) NULL DEFAULT NULL,
  `opacidade` VARCHAR(50) NULL DEFAULT NULL,
  `lombada` VARCHAR(5) NULL DEFAULT NULL,
  `medLombada` VARCHAR(50) NULL DEFAULT NULL,
  `bolAnulado` INT(11) NULL,
  `created_at` TIMESTAMP NOT NULL,
  `updated_at` TIMESTAMP NOT NULL,
  PRIMARY KEY (`idEspecificacao`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_unicode_ci;


