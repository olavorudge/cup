ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_unicode_ci;


-- -----------------------------------------------------
-- Table `cadastro_unico`.`produto`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `cadastro_unico`.`produto` (
  `idProduto` INT(11) NOT NULL AUTO_INCREMENT,
  `idAreaConhecimento` INT(11) NOT NULL,
  `idAnoEscolar` INT(11) NOT NULL,
  `idOrigem` INT(11) NOT NULL,
  `titulo` VARCHAR(350) NOT NULL,
  `tituloObra` VARCHAR(350) NULL DEFAULT NULL,
  `anoUso` INT(11) NULL DEFAULT '0',
  `anoLancamento` INT(11) NULL DEFAULT '0',
  `anoCicloVida` INT(11) NULL DEFAULT '0',
  `volume` INT(11) NULL DEFAULT '0',
  `numEdicao` VARCHAR(150) NULL DEFAULT NULL,
  `pegLA` VARCHAR(150) NULL DEFAULT NULL,
  `pegLP` VARCHAR(150) NULL DEFAULT NULL,
  `ISBN_LA` VARCHAR(150) NULL DEFAULT NULL,
  `ISBN_LP` VARCHAR(150) NULL DEFAULT NULL,
  `nomeContrato` VARCHAR(150) NULL DEFAULT NULL,
  `nomeCapa` VARCHAR(150) NULL DEFAULT NULL,
  `pseudonomio` VARCHAR(150) NULL DEFAULT NULL,
  `numContrato` INT(11) NULL DEFAULT '0',
  `dataAssinatura` DATE NULL DEFAULT NULL,
  `validadeContrato` DATE NULL DEFAULT NULL,
  `arquivado` TINYINT(1) NOT NULL,
  `bolAnulado` TINYINT(1) NULL,
  `created_at` TIMESTAMP NOT NULL,
  `updated_at` TIMESTAMP NOT NULL,
  PRIMARY KEY (`idProduto`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_unicode_ci;


